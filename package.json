{
  "name": "tamsui",
  "version": "v1.0.0",
  "description": "An Express/React universal application boilerplate",
  "main": "dist/index.js",
  "scripts": {
    "analyze": "webpack-bundle-analyzer stats.json",
    "analyze:dev": "run-s build:dev analyze",
    "analyze:prod": "run-s build:prod analyze",
    "prebuild:dev": "run-s clean build:static",
    "build:dev": "webpack --config webpack/config.js --profile --json > stats.json",
    "build:dist": "mkdir -p dist",
    "prebuild:prod": "run-s clean build:static",
    "build:prod": "NODE_ENV=prod webpack --config webpack/config.js --profile --json > stats.json",
    "prebuild:static": "npm run build:dist",
    "build:static": "cp -r static dist/static",
    "clean": "rm -f stats.json && rm -rf dist",
    "dev": "run-s build:dev run | pino-pretty -c -S",
    "lint": "eslint --ext .js,.jsx,.mjs,.ts,.tsx .",
    "prod": "run-s build:prod run | pino-pretty -c -S",
    "run": "node dist/index.js",
    "start": "run-s build:prod run",
    "test": "jest",
    "test:snapshot": "jest --updateSnapshot",
    "prewatch": "run-s clean build:static",
    "watch": "webpack --config webpack/config.js --watch | pino-pretty -c -S"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chichiwang/tamsui.git"
  },
  "keywords": [
    "javaScript",
    "react",
    "typescript"
  ],
  "author": "Chi-chi Wang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chichiwang/tamsui/issues"
  },
  "homepage": "https://github.com/chichiwang/tamsui#readme",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/express": "^4.17.21",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "babel-jest": "^29.7.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.8.1",
    "nodemon-webpack-plugin": "^4.8.2",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^11.0.0",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.72.0",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.1.0",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "express": "^4.19.2",
    "js-cookie": "^3.0.5",
    "pino": "^8.20.0",
    "pino-http": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "serve-static": "^1.16.2"
  }
}
